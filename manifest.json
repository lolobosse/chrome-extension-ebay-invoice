{
  "manifest_version": 2,
  "name": "Ebay Invoices",
  "description": "Creates invoices based on ebay website data",
  "version": "1.0",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://k2b-bulk.ebay.de/*"
      ],
      "js": [
        "jquery-3.5.1.min.js",
        "jspdf.min.js",
        "jspdf.plugin.autotable.js",
        "moment-with-locales.min.js",
        "main.js"
      ]
    }
  ]
}
